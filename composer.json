{
    "name": "ashokatree/atvslider",
    "type": "typo3-cms-extension",
    "description": "Responsive vertical slider with Title, Link, Subtitle, Description, Image.",
    "version": "10.0.0",
    "keywords": [
        "slider"
    ],
    "homepage": "https://ashokatree.net",
    "authors": [
        {
            "name": "Nirmalya Mondal",
            "email": "nirmalya.mondal@gmail.com",
            "role": "Developer"
        }
    ],
    "license": [
        "GPL-2.0-or-later"
    ],
    "minimum-stability": "stable",
    "require": {
        "typo3/cms-core": "^10.0.0"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "at_vslider"
        }
    },
    "autoload": {
        "psr-4": {
            "AshokaTree\\AtVslider\\": "Classes"
        }
    },
    "replace": {
        "ashokatree/atvslider": "self.version",
        "typo3-ter/atvslider": "self.version"
    }
}